<div class="member">

  <%= link_to member_path(member.id), target: "_blank" do %>
  <div class="details">
    <strong><%= member.username %></strong> <span class="post-metadata"> <i class="fa fa-map-marker"></i> <%= member.location %></span>
    <p><%= member.blurb %></p>
  </div>
  <% end %>
</div>
